{
  "name": "brainsafes-pro",
  "version": "1.0.0",
  "description": "Sistema completo de e-learning descentralizado con IA, becas, NFTs y marketplace",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:local": "hardhat run scripts/deploy.js --network localhost",
    "deploy:testnet": "hardhat run scripts/deploy.js --network arbitrumSepolia",
    "deploy:arbitrum": "hardhat run scripts/deploy.js --network arbitrum",
    "deploy:goerli": "hardhat run scripts/deploy.js --network arbitrumGoerli",
    "verify:testnet": "hardhat verify --network arbitrumSepolia",
    "verify:arbitrum": "hardhat verify --network arbitrum",
    "verify:goerli": "hardhat verify --network arbitrumGoerli",
    "node": "hardhat node",
    "clean": "hardhat clean",
    "coverage": "hardhat coverage",
    "lint": "solhint 'contracts/**/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "size": "hardhat size-contracts",
    "analyze": "slither .",
    "flatten": "hardhat flatten",
    "gas-report": "REPORT_GAS=true hardhat test",
    "arbitrum:bridge": "node scripts/bridge-assets.js",
    "arbitrum:status": "node scripts/check-arbitrum-status.js"
  },
  "keywords": [
    "blockchain",
    "solidity",
    "education",
    "elearning",
    "nft",
    "defi",
    "ai",
    "smart-contracts",
    "web3",
    "ethereum"
  ],
  "author": "BrainSafes Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
